{
  "name": "parallelizer-function",
  "version": "0.0.1.dev",
  "description": "A npm package for running javaScript function in a different Thread. This uses the Worker api",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "test": "jest",
    "semantic-release": "semantic-release",
    "prepack": "npm run build"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/josealejandro2928/parallelizer-function.git"
  },
  "keywords": [
    "npm",
    "javascript",
    "typescript",
    "esm",
    "cjs",
    "nodejs",
    "commonjs",
    "ecmascript",
    "worker",
    "parallalizer",
    "multithreading Js"
  ],
  "author": "Jose Alejandro Concepcion Alvarez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/josealejandro2928/parallelizer-function.git/issues"
  },
  "homepage": "https://github.com/josealejandro2928/parallelizer-function.git#readme",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/jest": "^29.2.6",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "semantic-release": "^19.0.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}